
BONUS
public int twoSum(int[]nums, int target) {
    	HashMap<Integer,Integer>map = new HashMap<Integer,Integer>();
    	int count = 0;
    	for (int num : nums) {
    		map.put(num, num);
    	}
    	for (int num : nums) {
    		int newtarget = target - num;
    		if (map.containsKey(newtarget) && map.get(newtarget) != newtarget) {
    			count++;
    		}
    	}
    	count = count/2;
    	return count;
}
